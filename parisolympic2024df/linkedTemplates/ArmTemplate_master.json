					"AzureDataLakeStorage4_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage4_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage5_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage5_properties_typeProperties_url')]"
					},
					"EventHttp_properties_typeProperties_url": {
						"value": "[parameters('EventHttp_properties_typeProperties_url')]"
					},
					"ScheduleHttp_properties_typeProperties_url": {
						"value": "[parameters('ScheduleHttp_properties_typeProperties_url')]"
					},
					"Torch_RouteHttp_properties_typeProperties_url": {
						"value": "[parameters('Torch_RouteHttp_properties_typeProperties_url')]"
					},
					"VenuesHttp_properties_typeProperties_url": {
						"value": "[parameters('VenuesHttp_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "parisolympic2024df_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'parisolympic2024df_ArmTemplate_0')]"
			]
		}
	]
}